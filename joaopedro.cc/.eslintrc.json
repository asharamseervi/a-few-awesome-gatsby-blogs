{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "parser": "babel-eslint",
  "rules": {
    "operator-linebreak": "off",
    "implicit-arrow-linebreak": "off",
    "no-confusing-arrow": "off",
    "react/prefer-stateless-function": "off",
    "arrow-parens": "off",
    "react/prop-types": "off",
    "react/no-danger": "off",
    "linebreak-style": 0,
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": [
          "Link"
        ],
        "specialLink": [
          "hrefLeft",
          "hrefRight",
          "to"
        ],
        "aspects": [
          "noHref",
          "invalidHref",
          "preferButton"
        ]
      }
    ],
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "object-curly-newline": 0
  },
  "settings": {
    "import/core-modules": []
  },
  "env": {
    "browser": true
  }
}
